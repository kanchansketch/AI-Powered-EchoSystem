spring:
  application:
    name: management-service
    
server:
  port: 8089

eureka:
  client:
    register-with-eureka: true
    fetch-registry: true
    serviceUrl:
      defaultUrl: htllp://localhost:8761/eureka
  instance:
    instance-id: ${spring.application.name}:${spring.application.instance_id:${random.value}}